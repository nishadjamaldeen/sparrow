                                    </vbm:Spots>
                                    <vbm:Circles>
                                        <vbm:items/>
                                    </vbm:Circles>
                                    <vbm:Routes>
                                        <vbm:items/>
                                    </vbm:Routes>
                                </vbm:vos>
                            </vbm:GeoMap>
